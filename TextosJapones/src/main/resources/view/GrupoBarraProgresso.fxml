<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXProgressBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="background" prefHeight="40.0" prefWidth="500.0" styleClass="background-Transparent" stylesheets="@../css/Dark_Theme.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.jisho.textosJapones.controller.GrupoBarraProgressoController">
   <children>
      <VBox layoutY="5.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label fx:id="lblProgresso" alignment="BOTTOM_LEFT" contentDisplay="BOTTOM" styleClass="progress-TextAzul" text="Titulo" wrapText="true">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <Label fx:id="lblTitulo" alignment="BOTTOM_LEFT" contentDisplay="BOTTOM" styleClass="progress-TextAzul" text="Processando" wrapText="true" />
         </children>
      </VBox>
      <JFXProgressBar fx:id="barraProgresso" prefHeight="7.0" styleClass="progress-barAzul" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />
   </children>
</AnchorPane>
