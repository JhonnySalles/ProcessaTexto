<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXSlider?>
<?import com.jfoenix.controls.JFXTextArea?>
<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<HBox fx:id="hbRoot" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" spacing="5.0" stylesheets="@../css/Dark_Theme.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.jisho.textosJapones.controller.MangasTextoCelulaController">
   <children>
      <AnchorPane fx:id="originalRoot" maxHeight="500.0" minHeight="400.0" prefHeight="400.0">
         <children>
            <ImageView fx:id="originalImagem" fitHeight="400.0" fitWidth="300.0" nodeOrientation="INHERIT" pickOnBounds="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
            <JFXSlider AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </JFXSlider>
         </children>
      </AnchorPane>
      <JFXTextArea fx:id="originalTextos" focusColor="#71afe5" labelFloat="true" prefWidth="200.0" unFocusColor="#106ebe" wrapText="true">
         <styleClass>
            <String fx:value="texto-stilo" />
            <String fx:value="background-Blue3" />
         </styleClass>
      </JFXTextArea>
      <AnchorPane fx:id="esquerdaRoot" maxHeight="500.0" minHeight="400.0" prefHeight="400.0">
         <children>
            <ImageView fx:id="esquerdaImagem" fitHeight="400.0" fitWidth="300.0" nodeOrientation="INHERIT" pickOnBounds="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
            <JFXSlider fx:id="esquerdaSlider" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </JFXSlider>
         </children>
      </AnchorPane>
      <AnchorPane fx:id="direitaRoot" maxHeight="500.0" minHeight="400.0" prefHeight="400.0">
         <children>
            <ImageView fx:id="direitaImagem" fitHeight="400.0" fitWidth="300.0" nodeOrientation="INHERIT" pickOnBounds="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
            <JFXSlider fx:id="direitaSlider" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </JFXSlider>
         </children>
      </AnchorPane>
      <JFXTextArea fx:id="vinculadoTextos" focusColor="#71afe5" labelFloat="true" prefWidth="200.0" unFocusColor="#106ebe" wrapText="true">
         <styleClass>
            <String fx:value="texto-stilo" />
            <String fx:value="background-Blue3" />
         </styleClass>
      </JFXTextArea>
   </children>
</HBox>
